<?xml version='1.0' encoding='utf-8'?>
<widget id="id.shakti.eretribusi_agen" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>E-Retribusi Agen</name>
    <description>
        aplikasi e-retribusi untuk agen
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        SHAKTI Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" height="57" platform="android" src="res/icon/android/logo1.png" width="57" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="skwas-cordova-plugin-datetimepicker" spec="^1.1.3" />
    <plugin name="cordova-plugin-clipboard2" spec="^0.1.0" />
    <plugin name="phonegap-plugin-barcodescanner" spec="~8.0.1" />
    <engine name="android" spec="^7.0.0" />
</widget>
